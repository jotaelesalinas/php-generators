{
    "name": "jotaelesalinas/php-rwgen",
    "type": "library",
    "description": "Read/write generators in PHP",
    "keywords": [
        "jotaelesalinas",
        "php-rwgen",
        "php",
        "rwgen",
        "io",
        "read-write",
        "read",
        "write",
        "rw",
        "generators",
        "input",
        "output",
        "producer",
        "consumer"
    ],
    "homepage": "https://github.com/jotaelesalinas/php-rwgen",
    "license": "MIT",
    "authors": [
        {
            "name": "JosÃ© Luis Salinas",
            "email": "jotaelesalinas@example.com",
            "homepage": "https://github.com/jotaelesalinas",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "phpunit/php-code-coverage": "^4"
    },
    "autoload": {
        "psr-4": {
            "JLSalinas\\RWGen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JLSalinas\\RWGen\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
