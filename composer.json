{
    "name": "jotaelesalinas/php-generators",
    "type": "library",
    "description": "Read/write generators in PHP",
    "keywords": [
        "php-generators",
        "php",
        "generators",
        "io",
        "read-write",
        "read",
        "write",
        "rw",
        "input",
        "output",
        "producer",
        "consumer"
    ],
    "homepage": "https://github.com/jotaelesalinas/php-generators",
    "license": "MIT",
    "authors": [
        {
            "name": "JosÃ© Luis Salinas",
            "email": "jotaelesalinas@example.com",
            "homepage": "https://github.com/jotaelesalinas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8",
        "league/csv": "^9.8"
    },
    "require-dev": {
        "php": ">=8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Generators\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Generators\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/ tests/",
        "format-win": "phpcbf --standard=psr2 --no-patch src/",
        "smells": "phpcs --standard=psr2 src/"
    },
    "config": {
        "sort-packages": true
    }
}
